# Copy this to .env and fill in your API keys
GEMINI_API_KEY=your_gemini_api_key_here

# Telegram Bot Configuration
TELEGRAM_API_ID= replace_with_api_id
TELEGRAM_API_HASH= api_hash
TELEGRAM_PHONE= phone_no

# Reddit API (PRAW)
# personal use script -> client_id
REDDIT_CLIENT_ID= client_id
# secret -> client_secret
REDDIT_CLIENT_SECRET= enter_client_secret
# Recommended descriptive UA; replace YOUR_REDDIT_USERNAME
REDDIT_USER_AGENT= enter_user_agent

# Discord Bot
DISCORD_BOT_TOKEN= bot_token


